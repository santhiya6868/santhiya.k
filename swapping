#include<stdio.h>

int main() {   
     int num1, num2;
     printf("Enter first number: ");
     scanf("%d", &num1);
     printf("Enter second number: ");
     scanf("%d", &num2);


     /*swapping*/  
     num1 = num1 + num2;
     num2 = num1 - num2;
     num1 = num1 - num2;

     printf("\nAfter swapping: ");
     printf("\n Value of First Number = %d", num1);
     printf("\n Value of Second Number = %d", num2);

     return 0;
}
